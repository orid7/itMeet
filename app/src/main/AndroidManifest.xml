<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfagroup.itmeetapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.INTERNET" />
        <activity android:name="com.alfagroup.itmeetapp.LoginActivity" />
        <activity android:name="com.alfagroup.itmeetapp.RegisterActivity" />
        <activity android:name="com.alfagroup.itmeetapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.alfagroup.itmeetapp.MoreDetailsActivity" />
        <activity android:name="com.alfagroup.itmeetapp.ChooseLang" />

    </application>



</manifest>